{
    "name": "challenger-platform/antiscanscanclub-laravel",
    "description": "A Laravel Package to Block Automated Scanners from Scanning your Site",
    "license": "MIT",
    "authors": [
        {
            "name": "noobSecurity",
            "email": "root@noobsec.org",
            "homepage": "https://noobsec.org"
        },
        {
            "name": "Tomas Stan",
            "email": "tomas.stan@enga.systems",
            "homepage": "https://challengerplatform.com"
        }
    ],
    "homepage": "https://github.com/challenger-platform/AntiScanScanClub-laravel",
    "keywords": ["Laravel", "AntiScanScanClub", "noobSecurity", "noobsec"],
    "require": {
        "illuminate/support": "^6 || ^7"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "challengerplatform\\AntiScanScanClub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "challengerplatform\\AntiScanScanClub\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "challengerplatform\\AntiScanScanClub\\AntiScanScanClubServiceProvider"
            ],
            "aliases": {
                "AntiScanScanClub": "challengerplatform\\AntiScanScanClub\\Facades\\AntiScanScanClub"
            }
        }
    }
}
